    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run create-conference1",
            "name": "Debug create-conference1 (local)",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "command": "npm start",
            "name": "Debug parse-server (local)",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/backend/cloud/"
        },
        {
            "name": "Launch convert-program-csv via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "convert-program-csv"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch tests via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "test",
                "--",
                "--runInBand",
                "--no-cache"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch init-test-data via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "init-test-data"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch db.js via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "init-app"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch test-db.js via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "init-test-data"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}